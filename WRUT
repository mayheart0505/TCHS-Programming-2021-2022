import java.util.*;
public class wrut {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int runs = scan.nextInt();
		
		for(int i = 0; i<runs;i++){
			int a = scan.nextInt();
			int b = scan.nextInt();
			int c = scan.nextInt();
			
			if(c == (a+b)){
				System.out.println("Correct!");
			}else{
				System.out.println("Wrut Row!");
			}
		}
	}

}
